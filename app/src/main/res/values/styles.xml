<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="StateView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="msv_animateViewChanges">false</item>
        <item name="msv_emptyView">@layout/layout_state_empty</item>
        <item name="msv_errorView">@layout/layout_state_error</item>
        <item name="msv_loadingView">@layout/layout_state_loading</item>
        <item name="msv_viewState">content</item>
    </style>

    <declare-styleable name="viewOutLineStrategy">
        <attr name="clip_radius" format="dimension"></attr>
        <attr name="clip_side" format="enum">
            <enum name="all" value="0"></enum>
            <enum name="left" value="1"></enum>
            <enum name="top" value="2"></enum>
            <enum name="right" value="3"></enum>
            <enum name="bottom" value="4"></enum>
        </attr>
    </declare-styleable>
</resources>